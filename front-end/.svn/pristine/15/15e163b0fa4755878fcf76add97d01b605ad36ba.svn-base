
function saveSuccessPrompt() {
    return wx.showToast({
        title: '保存成功',
        icon: 'success',
        duration: 1000,
    })
}
function saveFailPrompt() {
    return wx.showToast({
        title: '保存失败',
        icon: 'error',
        duration: 1000,
    })
}

module.exports = {
    saveSuccessPrompt: saveSuccessPrompt,
    saveFailPrompt: saveFailPrompt,
}