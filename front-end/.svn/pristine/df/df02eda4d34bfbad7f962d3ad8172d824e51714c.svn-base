Page({

	/**
	 * 页面的初始数据
	 */
	data: {
		gjsz:false,
		date: '请选择',
		isshow:false,
	},
	bindDateChange: function(e) {
    console.log('picker发送选择改变，携带值为', e.detail.value)
    this.setData({
      date: e.detail.value
    })
	},
	gjszchange(e){
		let val = e.detail.value
		console.log(val)

		this.setData({
			isshow:val
		})
	},
	/**
