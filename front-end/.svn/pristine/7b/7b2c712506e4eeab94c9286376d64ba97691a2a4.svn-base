.ft {
    float: left;
}

.fr {
    float: right;
}

.clearfloat::after {
    content: '';
    display: block;
    clear: both;
}

.zzc {
    width: 100%;
    height: 100%;
    background: #000000;
    opacity: 0.3;
    position: fixed;
    z-index: 99990;
    top: 0;
}

.qzbox {
    width: 85%;
    margin: 0 auto;
    background: #fff;
    top: 10%;
    position: fixed;
    z-index: 99991;
    left: 0;
    right: 0;
    border-radius: 10px 10px 10px 10px;
    padding-bottom: 20px;
}

.qusf {
    font-size: 24px;
    font-family: SimHei-Regular, SimHei;
    font-weight: 400;
    color: #333333;
    padding: 25px 0 0 24px;
}

.textts {
    font-size: 14px;
    color: #332d2d;
    margin: 0 20px;
    margin-top: 10px;
}


.pomis {
    height: 35px;
    line-height: 35px;
    width: 40% !important;
    display: flex;
    justify-content: center;
    align-items: center;
}

.tijsq {
    height: 46px;
    background: #4376E4;
    opacity: 1;
    border-radius: 4px;
    width: 94%;
    margin: 0 auto;
    color: #FFFFFF;
    text-align: center;
    line-height: 46px;
    margin-top: 17px;
}