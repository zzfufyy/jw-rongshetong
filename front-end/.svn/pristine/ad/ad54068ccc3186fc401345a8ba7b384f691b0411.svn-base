.container {
    background: #fff;
    padding-bottom: 0px;
}

.flex-box {
    min-height: 64px;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column wrap;
    text-align: center;
    border-bottom: 1px solid #F2F2F2;
    padding-bottom: 5px;
}

.item-top {
    width: 100%;
    font-size: 14px;
    font-family: Source Han Sans CN-Regular, Source Han Sans CN;
    font-weight: 400;
    color: #C1C1C1;
}

.item-bottom {
    width: 100%;
    font-size: 20px;
    font-family: Source Han Sans CN-Regular, Source Han Sans CN;
    font-weight: 400;
    color: #333333;
    margin-top: 5px;
}

.width-per-50 {
    width: 50%;
}

.flex-row-start {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: row wrap;
    text-align: center;
    min-height: 64px;
    border-bottom: 1px solid #F2F2F2;
    padding-bottom: 5px;
    width: 100%;
}

.button-big-save {
    height: 46px;
    background: #4376E4;
    opacity: 1;
    border-radius: 4px;
    width: 94%;
    margin: 0 auto;
    color: #FFFFFF;
    text-align: center;
    line-height: 46px;
    margin-top: 17px;
}