/* 遮罩层 */
.zzc {
    width: 100%;
    height: 100%;
    background: #000000;
    opacity: 0.3;
    position: fixed;
    z-index: 99990;
    top: 0;
}

.qwzwbox {
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    right: 0;
    margin: 0 auto;
    z-index: 99991;
    background: #fff;
    padding-bottom: 14px;
}

.INinputheader {
    width: 100%;
    margin: 0 auto;
    height: 30px;
    /* background: rgba(255,255,255,1); */
    border-radius: 7px;
    font-size: 14px;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.INsearchicon {
    position: absolute;
    left: 5%;
    top: 4px;
}

.weui-input {
    height: 30px;
    line-height: 30px;
    text-align: left;
    padding-left: 30px;
    letter-spacing: 0px;
    background-color: #F5F5F5;
    border-radius: 30px;
    color: #333;
    width: 85%;
    display: inline-block;
}

.middlebox {
    display: flex;
    padding-bottom: 20px;
    flex-wrap: wrap;
    margin-top: 20px;
}

.wantjob {
    padding: 7px 14px;
    border-radius: 3px;
    margin: 5px 5px 0 5px;
    /* background: #fff; */
    color: #333;
    border-radius: 3px;
}

.hascs {
    margin: 5px 5px 0 5px;
    padding: 7px 14px;
}

.active {
    background: #EAF2FF;
    color: #3384FD;
}

.tijsq {
    height: 46px;
    background: #4376E4;
    opacity: 1;
    border-radius: 4px;
    width: 94%;
    margin: 0 auto;
    color: #FFFFFF;
    text-align: center;
    line-height: 46px;
    margin-top: 17px;
}

.btn_box .tijsq {
    width: 43%;
    margin: 0;
}

.btn_box {
    display: flex;
    align-items: center;
    justify-content: space-evenly;
}

.quxiao {
    margin-left: 10px;
    color: #2B7FFD;
}

.btn_box .quxiao {
    background: #FFFFFF;
    border: 1px solid #999999;
    color: #999999;

}