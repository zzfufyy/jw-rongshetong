.creatbox_top {
	height: 48px;
	line-height: 48px;
	box-shadow: 0px 2px 2px 1px rgba(153, 153, 153, 0.16);
	padding: 0 20px;
}
.mycreat {
	font-size: 15px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #111111;
}
.mycreat::after{
	content: "";display: block;width: 30px;height:2px;background-color: #111111;margin: 0 auto;margin-top: -2px;
}
.pd15{
	padding-left: 15px;
}
.tpbox{
	margin: 15px 15px;margin-bottom: 0;border: 1px dashed #B4B4B4;position: relative;
}
.tit{
	padding-top: 15px;font-size: 14px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #333333;padding-bottom: 10px;
}
.ytp{
	padding-bottom: 14px;
}
.ytpbox{
	font-size: 12px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #333333;margin-right: 11px;
}
.jzrq{
	font-size: 12px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #999999;
}
.wytp{
	font-size: 12px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #3082FD;
	padding-top:13px ;padding-bottom: 15px;
}
.tpimg{
	width: 22px;height: 22px;vertical-align: middle;
}
.tpimgbox{
	position: absolute;top: -4px;right: 10px;
}
.tpz{
	font-size: 12px;
	font-family: Source Han Sans CN-Medium, Source Han Sans CN;
	font-weight: bold;
	color: #F8424C;
}
.yjs{
	color: #999999;
}
