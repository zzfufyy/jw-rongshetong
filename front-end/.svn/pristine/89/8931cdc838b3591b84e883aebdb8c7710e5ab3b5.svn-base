 /* .table view{display: inline-block;} */
 .tr {
	display: flex;
	width: 100%;
	justify-content: center;
	height: 64rpx;
	align-items: center;
	background: rgba(247, 252, 253, 0.39);
 }
 .tr:last-of-type{border-bottom: 1px solid #D9DDDC;}
 .bg-g{
	background: #EEF1F6;
 }
 .td {
	justify-content: center;
	text-align: center;
	font-size: 12px;    height: 64rpx;
	line-height: 64rpx;
	border-right: 1px solid #D9DDDC;
	border-top: 1px solid #D9DDDC;
	/* border-left: 1px solid #D9DDDC; */
	display: flex;
	align-items: center;
	font-size: 12px;
	overflow: hidden;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	padding-left:15rpx ;padding-right: 15rpx;
 }
.td:nth-child(1){
	width: 110px;text-align: left;padding-left: 5px;border-left:1px solid #D9DDDC ;
	text-overflow: ellipsis;
	white-space: break-spaces;
	line-height: 64rpx;
	text-align: center;
	padding-right: 5px;border-top: 1px solid #D9DDDC;
}

.th {
	justify-content: center;
	text-align: center;
	font-size: 12px;    height: 64rpx;
	line-height: 64rpx;
	border-right: 1px solid #D9DDDC;
	border-top: 1px solid #D9DDDC;
	/* border-left: 1px solid #D9DDDC; */
	display: flex;
	align-items: center;
	font-size: 12px;
	overflow: hidden;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	padding-left:15rpx ;padding-right: 15rpx;
	background: #E5E5E5; 
 }
