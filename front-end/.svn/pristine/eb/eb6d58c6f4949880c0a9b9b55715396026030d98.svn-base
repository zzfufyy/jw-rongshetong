/* pages/tpewm/tpewm.wxss */
.contbox{
	text-align: center;
	margin-top: 51px;
	width: 270px;
	height: 330px;
	margin: 51px auto;
	border: 1px solid #E8E8E8;
	box-shadow: 0px 3px 6px 1px rgba(153, 153, 153, 0.16);
	display: flex;
	/* align-items: center; */
	justify-content: center;
	position: relative;
}
.ca{
	position: absolute;
	bottom: 20px;font-size: 14px;
	font-family: Source Han Sans CN-Regular, Source Han Sans CN;
	font-weight: 400;
	color: #999999;
}

.imagePathBox {
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.7);
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 10;
}
.closeimgbtn {
	width: 32px;
	height: 32px;
	position: absolute;
	right: 6%;
	top: 25rpx;
	z-index: 99;
}
.shengcheng {
	width: 80%;
	height: 80%;
	position: fixed;
	top: 50rpx;
	left: 50%;
	margin-left: -40%;
	z-index: 10;
}
.baocun {
	display: block;
	width: 80% !important;
	height: 90rpx;
	padding: 0;
	line-height: 90rpx;
	text-align: center;
	position: fixed;
	bottom: 50rpx;
	left: 0;
	color: #fff;
	font-size: 32rpx;
	border-radius: 8rpx;
	background: #4376E4;
	right: 0;
}




